        <li><%= image_tag 'mambo.jpg', class: 'border-rounded' %></li>
        <li><%= image_tag 'salon-milonga.jpg', class: 'border-rounded' %></li>
        <li><%= image_tag 'swing2.jpg', class: 'border-rounded' %></li>
        <li><%= image_tag 'contempo-2.jpg', class: 'border-rounded' %></li>
